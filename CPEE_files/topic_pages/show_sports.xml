<?xml version="1.0" encoding="UTF-8"?>
<testset xmlns="http://cpee.org/ns/properties/2.0">
  <executionhandler>ruby</executionhandler>

  <dataelements/>

  <endpoints>
    <timeout>https://cpee.org/services/timeout.php</timeout>
    <subprocess>https://cpee.org/flow/start/url/</subprocess>
    <frames_display>https-put://cpee.org/out/frames/news/</frames_display>
    <waitqr>https-get://lehre.bpm.in.tum.de/~go56sew/waitqr/initiate.php</waitqr>
    <frames_init>https-post://cpee.org/out/frames/news/</frames_init>
    <tech_update>https://lehre.bpm.in.tum.de/~go56sew/jsonpasser/tech_parser.php?q=tech</tech_update>
    <sports_update>https://lehre.bpm.in.tum.de/~go56sew/jsonpasser/sports_parser.php?q=sports</sports_update>
  </endpoints>

  <attributes>
    <guarded>none</guarded>
    <modeltype>CPEE</modeltype>
    <theme>extended</theme>
    <guarded_id/>
    <info>show_sports</info>
    <creator>Christine Ashcreek</creator>
    <author>Christine Ashcreek</author>
    <model_uuid>af813d79-c6c1-4f1d-8f51-f0bf915b269f</model_uuid>
    <model_version/>
    <design_dir>Teaching.dir/Prak.dir/TUM-Prak-25-SS.dir/Raue_Ole.dir/topic_pages.dir/</design_dir>
    <design_stage>development</design_stage>
  </attributes>

  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">

      <!-- Update sports in backend -->
      <call id="a3" endpoint="sports_update">
        <parameters>
          <label>Update sports in backend</label>
          <method>:post</method>
          <arguments/>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result"/>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations/>
        <documentation/>
      </call>

      <!-- Show Sports Page -->
      <call id="a1" endpoint="frames_display">
        <parameters>
          <label>Show Sports Page</label>
          <arguments>
            <type>set</type>
            <lx>0</lx>
            <ly>0</ly>
            <x_amount>5</x_amount>
            <y_amount>7</y_amount>
            <button/>
            <style/>
            <urls>
              <urls_item>
                <lang>de-at</lang>
                <url>https://lehre.bpm.in.tum.de/~go56sew/categories/sports.html</url>
              </urls_item>
            </urls>
            <default>
              <titles>!JSON.generate(data.test)</titles>
            </default>
          </arguments>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result"/>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations/>
        <documentation/>
      </call>

      <!-- Show Navigation Bar -->
      <call id="a2" endpoint="frames_display">
        <parameters>
          <label>Show Navigation Bar</label>
          <arguments>
            <type>set</type>
            <lx>0</lx>
            <ly>7</ly>
            <x_amount>5</x_amount>
            <y_amount>1</y_amount>
            <button/>
            <style/>
            <urls>
              <urls_item>
                <lang>de-at</lang>
                <url>https://lehre.bpm.in.tum.de/~go56sew/categories/sports_navigation.html</url>
              </urls_item>
            </urls>
            <default/>
          </arguments>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result"/>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations/>
        <documentation/>
      </call>

    </description>
  </description>

  <transformation>
    <description type="copy"/>
    <dataelements type="none"/>
    <endpoints type="none"/>
  </transformation>
</testset>
